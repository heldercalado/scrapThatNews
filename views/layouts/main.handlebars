		})
		var d = new Date();
        $(".bodymid").append("<h1 class='midtext'>" + d + "</h1>")

		$("body").on("click" ,".note" , function(){
			id = $(this).data("articleid");
			console.log($(`#boxnote${id}`).css("display"));
